# Configuración del Backend
ENVIRONMENT=development
BACKEND_URL=http://172.29.228.80:8001

# Base de Datos (MySQL en la nube o PostgreSQL local)
# MySQL en la nube (TiDB, PlanetScale, Railway, etc.):
DATABASE_URL=mysql+pymysql://user:password@host.railway.app:3306/eventos_db
# O PostgreSQL local:
# DATABASE_URL=postgresql://postgres:postgres@localhost:5432/eventos_db

# APIs de Eventos
EVENTBRITE_API_KEY=your_eventbrite_api_key_here
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here
MEETUP_API_KEY=your_meetup_api_key_here

# APIs de IA (Sistema Multi-Provider con Fallback Automático)
# Provider preferido (grok, groq, gemini, perplexity, openrouter)
PREFERRED_AI_PROVIDER=grok

# Grok (xAI - Elon Musk) - Modelo Grok-4
GROK_API_KEY=your_grok_api_key_here

# Groq - Ultra rápido, 14,400 requests/día gratis
GROQ_API_KEY=your_groq_api_key_here

# Google Gemini - 250 requests/día gratis
GEMINI_API_KEY=your_gemini_api_key_here

# Perplexity AI - Búsqueda web en tiempo real
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# OpenRouter - Acceso a múltiples modelos (GPT-4, Claude, etc.)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Legacy (opcional)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# APIs de Integración Social
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
INSTAGRAM_ACCESS_TOKEN=your_instagram_token

# APIs de Transporte
UBER_API_KEY=your_uber_api_key
CABIFY_API_KEY=your_cabify_api_key

# Notificaciones Push
ONESIGNAL_APP_ID=your_onesignal_app_id
ONESIGNAL_API_KEY=your_onesignal_api_key

# Google Services
GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Cache Redis (opcional)
REDIS_URL=redis://localhost:6379
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here
GOOGLE_REDIRECT_URI=http://localhost:8001/auth/google/callback
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=1440